<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sofyun.admin.mapper.ProjectMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sofyun.admin.domain.Project">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="en_name" property="enName" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="owner_name" property="ownerName" />
        <result column="remark" property="remark" />
        <result column="status" property="status" />
        <result column="build_state" property="buildState" />
        <result column="template" property="template" />
        <result column="template_name" property="templateName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, en_name, create_time, update_time, owner_name, remark, status, build_state, template, template_name
    </sql>

</mapper>
